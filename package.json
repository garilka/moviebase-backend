{
  "type": "module",
  "scripts": {
    "start": "nodemon --env-file=.env ./src/server.ts",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "@types/lodash": "^4.17.4",
    "express": "^4.19.2",
    "lodash": "^4.17.21",
    "prisma": "^5.14.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "eslintConfig": {
    "extends": [
      "prettier"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "eqeqeq": "error",
      "no-console": "warn"
    }
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always"
  }
}
